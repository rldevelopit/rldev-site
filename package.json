{
  "name": "html-sass-js-starter",
  "version": "1.0.0",
  "description": "A simple HTML, Sass, and JavaScript starter theme",
"scripts": {
    "build:css": "sass scss:css --no-source-map",
    "watch:css": "sass --watch scss:css",
    "serve": "live-server --watch=css,*.html,js",
    "dev": "concurrently \"pnpm run watch:css\" \"pnpm run serve\""
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "live-server": "^1.2.2",
    "sass": "^1.0.0"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
